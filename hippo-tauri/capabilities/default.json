{
  "identifier": "default",
  "description": "Default capabilities for Hippo",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-spawn",
    {
      "identifier": "shell:allow-open",
      "allow": [{ "cmd": "qdrant" }]
    },
    "fs:default",
    "fs:allow-read-text-file",
    "fs:read-all"
  ]
}
