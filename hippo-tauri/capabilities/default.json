{
  "identifier": "default",
  "description": "Default capabilities for Hippo",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:tray:default",
    "core:tray:allow-new",
    "core:tray:allow-set-icon",
    "core:tray:allow-set-menu",
    "core:menu:default",
    "core:menu:allow-new",
    "core:menu:allow-popup",
    "dialog:default",
    "shell:default",
    "fs:default",
    "fs:allow-read-text-file",
    "fs:read-all"
  ]
}
